{
  "name": "code-generator-dusk",
  "displayName": "code generator dusk",
  "icon": "resources/icon.png",
  "publisher": "clovinr",
  "description": "代码生成器的黄昏，新人少用，老人慎用。",
  "license": "Mulan PSL v2",
  "homepage": "https://github.com/CLovinr/code-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/CLovinr/code-generator.git"
  },
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "code generator",
    "代码生成器"
  ],
  "main": "./dist/extension.js",
  "galleryBanner": {
    "color": "#6666ff",
    "theme": "dark"
  },
  "scripts": {
    "compile": "pnpm check-types && webpack --mode none",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:webpack": "webpack --watch --mode none",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "pnpm package",
    "package": "pnpm check-types && webpack --mode production",
    "vsce:package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.18",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/pg": "^8.15.4",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "mariadb": "^3.4.2",
    "mysql2": "^3.14.1",
    "pg": "^8.16.2",
    "sequelize": "^6.37.7",
    "tedious": "^18.6.1"
  },
  "extensionDependencies": [
    "mrmlnc.vscode-json5"
  ],
  "activationEvents": [],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "代码生成器",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
          "type": "webview",
          "id": "extension.sidebar",
          "name": "代码生成器",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "languages": [
      {
        "id": "jstpl",
        "aliases": [
          "JSTPL",
          "jstpl"
        ],
        "extensions": [
          ".jstpl",
          ".jsin"
        ],
        "configuration": "./configs/language-configuration.json",
        "semanticTokenTypes": [
          {
            "name": "scriptlet",
            "superType": "keyword"
          },
          {
            "name": "scriptlet2",
            "superType": "keyword"
          },
          {
            "name": "expression",
            "superType": "variable"
          },
          {
            "name": "directive",
            "superType": "keyword"
          }
        ],
        "semanticTokenModifiers": [
          {
            "name": "embedded",
            "superType": "modifier"
          }
        ]
      }
    ],
    "grammars": [
      {
        "language": "jstpl",
        "scopeName": "source.jstpl",
        "path": "./configs/jstpl.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "JS Template Generator",
      "properties": {
        "jstpl.defaultTextColor": {
          "type": "color",
          "description": "jstpl文件中非标签文本的颜色"
        },
        "jstpl.defaultTagColor": {
          "type": "color",
          "description": "jstpl文件中标签文本的颜色"
        }
      }
    },
    "colors": [
      {
        "id": "jstpl.defaultTextColor",
        "description": "jstpl文件中非标签文本的颜色",
        "defaults": {
          "dark": "#979797bc",
          "light": "#6161619f",
          "highContrast": "#9f9b9b"
        }
      },
      {
        "id": "jstpl.defaultTagColor",
        "description": "jstpl文件中标签文本的颜色",
        "defaults": {
          "dark": "#f5dd64",
          "light": "#474618",
          "highContrast": "#ba9555"
        }
      }
    ]
  }
}
