{
  local: "./local.json5",
  tplName: "MybatisPlus",
  baseOutDir: "./code-out",
  encoding: "utf-8",
  outEncoding: "utf-8",
  onStart: "\
    utils.onStart();\
  ",
  ui: {
    attr: "formState",
    labelWidth: 6, // 1~30
    params: [
      {
        var: "basePackageName",
        enable: true,
        label: "基础包名称",
        type: "textfield",
        default: "com.test.demo",
        required: true,
        maxlength: 64,
        title: "",
      },
      {
        var: "subName",
        label: "子包名称",
        type: "textfield",
        default: "mysub",
        required: true,
        maxlength: 64,
        title: "",
      },
      {
        var: "author",
        label: "作者",
        type: "textfield",
        default: "chenyg",
        required: true,
        maxlength: 64,
        title: "",
      },
      {
        var: "java1",
        label: "复选框",
        type: "checkbox",
        default: {
          controller: true,
          service: true,
        },
        required: false,
        options: [
          { text: "Controller", value: "controller" },
          { text: "Service", value: "service", "title": "服务层" },
        ],
      },
      {
        var: "java2",
        label: "",
        type: "checkbox",
        default: {
          entity: true,
          dao: true,
        },
        required: false,
        options: [
          { text: "Entity", value: "entity", "title": "实体" },
          { text: "Dao", value: "dao", "title": "" },
        ],
      },
      {
        var: "dropdownVar",
        label: "下拉框",
        type: "dropdown",
        default: "",
        required: false,
        title: "下拉框组件",
        options: [
          { text: "选项1", value: "option1", "title": "选项1提示" },
          { text: "选项2", value: "option2", "title": "选项2提示" },
        ],
      },
      {
        var: "fieldVar",
        label: "单行输入框",
        type: "textfield",
        default: "",
        required: false,
        maxlength: 128,
        title: "",
      },
      {
        var: "areaVar",
        label: "多行输入框",
        type: "textarea",
        default: "",
        required: false,
        maxlength: 512,
        title: "",
      },
    ],
  },
  databases: {
    current: "local-sqlite",
    items: {
      "local-mysql": {
        type: "mysql",
        enable: true,
        initSqls: ["SET NAMES utf8mb4"],
        options: {
          host: "localhost",
          port: 3306,
          username: "user",
          password: "password",
          database: "demo",
          ssl: false,
          dialectOptions: {
            // https://github.com/sidorares/node-mysql2
            insecureAuth: false,
          },
        },
      },
      'local-mariadb': {
        type: 'mariadb',
        enable: true,
        initSqls: ['SET NAMES utf8mb4'],
        options: {
          host: 'localhost',
          port: 4306,
          username: 'user',
          password: 'password',
          database: 'demo',
          ssl: false,
          dialectOptions: {
            insecureAuth: false
          }
        }
      },
      "local-pg": {
        type: "postgres",
        enable: true,
        initSqls: [],
        options: {
          host: "localhost",
          port: 5432,
          username: "postgres",
          password: "123456",
          database: "postgres",
          schema: "public",
          ssl: false,
        },
      },
      "local-mssql": {
        type: "mssql",
        enable: true,
        initSqls: [],
        options: {
          host: "localhost",
          port: 1433,
          username: "sa",
          password: "123456",
          database: "demo",
          schema: "dbo",
          ssl: false,
          dialectOptions: {
            options: {
              trustServerCertificate: true,
              encrypt: false,
            },
            authentication: {},
          },
        },
      },
      "local-sqlite": {
        type: "sqlite",
        enable: true,
        initSqls: [],
        options: {
          storage: "./sample-files/test-sqlite3.db",
        },
      },
    },
  },
  attrs: {
    removePrefix: "test_",
    typeMappers: [
      {
        matchType: "REGEX",
        columnType: "varchar(\\(\\d+\\))?",
        javaType: "java.lang.String",
      },
      {
        matchType: "REGEX",
        columnType: "char(\\(\\d+\\))?",
        javaType: "java.lang.String",
      },
      {
        matchType: "REGEX",
        columnType: "(tiny|medium|long)*text",
        javaType: "java.lang.String",
      },
      {
        matchType: "REGEX",
        columnType: "decimal(\\(\\d+,\\d+\\))?",
        javaType: "java.lang.Double",
      },
      {
        matchType: "ORDINARY",
        columnType: "integer",
        javaType: "java.lang.Integer",
      },
      {
        matchType: "REGEX",
        columnType: "(tiny|small|medium)*int(\\(\\d+\\))?",
        javaType: "java.lang.Integer",
      },
      {
        matchType: "ORDINARY",
        columnType: "int4",
        javaType: "java.lang.Integer",
      },
      {
        matchType: "ORDINARY",
        columnType: "int8",
        javaType: "java.lang.Long",
      },
      {
        matchType: "REGEX",
        columnType: "bigint(\\(\\d+\\))?",
        javaType: "java.lang.Long",
      },
      {
        matchType: "ORDINARY",
        columnType: "date",
        javaType: "java.util.Date",
      },
      {
        matchType: "ORDINARY",
        columnType: "datetime",
        javaType: "java.util.Date",
      },
      {
        matchType: "ORDINARY",
        columnType: "timestamp",
        javaType: "java.util.Date",
      },
      {
        matchType: "ORDINARY",
        columnType: "time",
        javaType: "java.time.LocalTime",
      },
      {
        matchType: "ORDINARY",
        columnType: "boolean",
        javaType: "java.lang.Boolean",
      },
    ],
  },
}
